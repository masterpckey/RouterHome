
config smartdns
	option server_name 'smartdns'
	option tcp_server '1'
	option dualstack_ip_selection '0'
	option prefetch_domain '1'
	option serve_expired '1'
	option rr_ttl_min '300'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_rule_soa '0'
	option seconddns_no_dualstack_selection '0'
	option seconddns_no_cache '0'
	option coredump '0'
	option ipv6_server '0'
	option cache_size '15000'
	option seconddns_tcp_server '1'
	option port '5333'
	option seconddns_server_group 'home'
	option seconddns_port '5335'
	option seconddns_no_speed_check '0'
	option force_aaaa_soa '1'
	option enabled '1'
	option seconddns_enabled '1'
	option redirect 'redirect'
	list old_redirect 'redirect'
	list old_port '5333'
	list old_enabled '1'

config server
	option ip '114.114.114.114'
	option type 'tcp'
	option name '114DNS'
	option enabled '1'

config server
	option type 'udp'
	option ip '114.114.114.114'
	option name '114DNS'
	option enabled '1'

